      skos:definition "A Point is defined by a single coordinate tuple. It is represented by a gml:Point element,\"OGC 07-144r3, cl. 6.3.11\""@en ;

      skos:notation "GM_Point"@en ;
      skos:prefLabel "Point"@en .

